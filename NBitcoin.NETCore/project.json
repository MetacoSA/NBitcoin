{
  "version" : "1.0.0.0",
  "name": "NBitcoin",
  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "NETStandard.Library": "1.6.0",
    "System.Collections.NonGeneric": "4.0.1",
    "System.Globalization.Extensions": "4.0.1",
    "System.Net.NameResolution": "4.0.0",
    "System.Net.Requests": "4.0.11",
    "System.Threading.Tasks": "4.0.11",
    "System.Threading.Tasks.Parallel": "4.0.0",
    "System.Threading.Thread": "4.0.0"
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
	"outputName": "NBitcoin",
    "define": [
      "DEBUG",
      "TRACE",
      "NETCORE",
      "NOTRACESOURCE"
    ],
    "compile": {
      "include": [
        "../NBitcoin/BIP32/*.cs",
        "../NBitcoin/BIP38/*.cs",
        "../NBitcoin/BIP39/*.cs",
        "../NBitcoin/BitcoinCore/*.cs",
        "../NBitcoin/BouncyCastle/**/*.cs",
        "../NBitcoin/BuilderExtensions/*.cs",
        "../NBitcoin/Crypto/*.cs",
        "../NBitcoin/Crypto/**/*.cs",
        "../NBitcoin/DataEncoders/*.cs",
        "../NBitcoin/OpenAsset/*.cs",
        "../NBitcoin/Payment/*.cs",
        "../NBitcoin/Policy/*.cs",
        "../NBitcoin/Protobuf/*.cs",
        "../NBitcoin/Protocol/*.cs",
        "../NBitcoin/Protocol/**/*.cs",
        "../NBitcoin/RPC/*.cs",
        "../NBitcoin/SPV/*.cs",
        "../NBitcoin/Stealth/*.cs",
        "../NBitcoin/Utils/*.cs",
        "../NBitcoin/*.cs",
        "../NBitcoin.Portable/**/*.cs",
        "../NBitcoin.Portable/*.cs"
      ],
      "exclude": [
        "../NBitcoin/MedianFilter1.cs",
		"../NBitcoin/RandomUtils.partial.cs",
        "../NBitcoin.Portable/Android.partial.cs",
        "../NBitcoin.Portable/Payment/BitcoinUrlBuilder.cs"
      ]
    }
  }
}